{ "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [ 
  { "type": "prompt",
    "alignment": "left",
    "segments": [ 
    { "type": "os",
      "style": "plain",
      "template": " {{if .WSL}}WSL @ {{end}}\uf17a \ue0b1",
      "background": "#546E7A",
      "foreground": "#26C6DA" 
    }, 
    { "type": "root",
      "style": "plain",
      "template": " \uf0e7 ",
      "background": "#546E7A",
      "foreground": "#26C6DA" 
    }, 
    { "type": "path",
      "style": "powerline",
      "template": " {{.Path}}",
      "background": "#546E7A",
      "foreground": "#D4E157",
      "properties": { 
        "style": "mixed", 
        "home_icon": "\uf4e2 "
    } }, 
    { "type": "python", 
      "style": "plain",
      "template": "<#26C6DA>\ue0b1</> \ue606 {{.Major}}.{{.Minor}}{{if .Error}} {{.Error}}{{else}}{{if .Venv}}({{.Venv}}){{end}}{{end}}<transparent></>",
      "background": "#546e7a",
      "foreground": "#D4E157"
    }, 
    { "type": "git",
      "style": "plain",
      "template": "<#26C6DA>\ue0b1</> \ue65d {{.HEAD}}",
      "background": "#546E7A",
      "foreground": "#D4E157" 
    }, 
    { "type": "text",
      "style": "plain",
      "template": "\ue0b0",
      "background": "transparent",
      "foreground": "#546E7A"
  } ] }, 
  { "type": "prompt",
    "alignment": "right",
    "segments": [ 
    { 
      "type": "text",
      "style": "plain",
      "template": "\ue0b2",
      "foreground": "#546E7A"
    },
    { "type": "git",
      "style": "plain",
      "background": "#546E7A",
      "foreground": "#D4E157",
      "template": "\ueba1{{if .BranchStatus }} {{.BranchStatus}}{{end}}{{if .Working.Changed}} \uf044 {{.Working.String}}{{end}}{{if and (.Working.Changed) (.Staging.Changed)}} |{{end}}{{if .Staging.Changed}} \uf046 {{.Staging.String}}{{end}}{{if gt .StashCount 0}} \ueb4b {{.StashCount}}{{end}} <#26C6DA>\ue0b3</>",
      "properties": {
        "branch_icon": "\uf126 ",
        "fetch_status": true,
        "fetch_stash_count": true,
        "fetch_upstream_icon": true
    } },  
    { "type": "time",
      "style": "plain",
      "template": "{{.CurrentDate | date .Format}} \uf017 ",
      "background": "#546E7A",
      "foreground": "#D4E157",
      "properties": { "time_format": "03:04 PM" } 
  } ] }, 
  { "type": "prompt",
    "alignment": "left",
    "newline": true,
    "segments": [ 
    { "type": "status",
      "style": "plain",
      "template": "\u276f ",
      "foreground": "#D4E157",
      "foreground_templates": [ "{{if gt .Code 0}}#FF5252{{end}}" ],
      "properties": { "always_enabled": true } 
} ] } ] }
